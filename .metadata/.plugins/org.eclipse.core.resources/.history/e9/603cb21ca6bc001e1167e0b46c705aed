package br.com.alura.notas;

public class TestaMerge {
	
	public static void main(String[] args) {
		
		Nota[] notasDoMauricio = {
			new Nota("Vinicius", 4),
			new Nota("Mariana", 8),
			new Nota("Paulo", 9),
			new Nota("Carlos", 9.5)
		};
		
		Nota[] notasDoALberto = {
				new Nota("Juliana", 2),
				new Nota("Jonas", 6),
				new Nota("Lucia", 7),
				new Nota("Ana", 8.5),
				new Nota("Guilherme", 10)
			};
		
		
		Nota[] rank = junta(notasDoMauricio, notasDoALberto);
		
		for(Nota nota : rank) {
			System.out.println(nota.getAluno());
		}
	}
}
